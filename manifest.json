{
  "manifest_version": 3,
  "name": "Simplifi Extension",
  "version": "1.0",
  "description": "Improvements to Simplifi",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["https://simplifi.quicken.com/*"],
      "js": ["content.js"]
    }
  ]
}
